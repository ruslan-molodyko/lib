<table class="table">
    <tr>
        {% for field in fieldContainer.all %}
            {% set label = field.label %}
            <th>
                {% if not label.isSortable %}
                    {{ label.label }}
                {% else %}
                    {{ knp_pagination_sortable(pagination, label.label, label.entityAlias) }}
                {% endif %}
            </th>
        {% endfor %}
    </tr>
    {% for list in pagination %}
        <tr>
            {% for field in list %}
                <td>
                    {{ link_collection_start(field, {"class": "ccc"}) }}
                        {{ field.value }}
                    {{ link_collection_end(field) }}
                </td>
            {% endfor %}
        </tr>
    {% endfor %}
</table>
<div class="navigation">
    {{ knp_pagination_render(pagination) }}
</div>
