<table class="table">
    <tr>
        {% for field in fieldContainer.all %}
            {% set label = field.label %}
            <th>
                {% if not label.isSortable %}
                    {{ label.label }}
                {% else %}
                    {{ knp_pagination_sortable(pagination, label.label, label.entityAlias) }}
                {% endif %}
            </th>
        {% endfor %}
    </tr>
    {% for list in pagination %}
        <tr>
            {% for field in list %}
                <td>
                    {% if field.link.isLinked %}
                        <a href="
                        {% if field.link.isCustomRoute %}
                            {% if field.link.route is iterable %}
                                {{ path(field.link.route|first, field.link.route[1]) }}">
                            {% else %}
                                {{ path(field.link.route) }}">
                            {% endif %}
                        {% else %}
                            {{ path(field.link.route, {'name': context.get('current_map_id'), 'id': list.id }) }}">
                        {% endif %}
                    {% endif %}
                    {{ field.value }}
                    {% if field.link.isLinked %}</a>{% endif %}
                </td>
            {% endfor %}
        </tr>
    {% endfor %}
</table>
<div class="navigation">
    {{ knp_pagination_render(pagination) }}
</div>
